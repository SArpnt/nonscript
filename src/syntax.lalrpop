use std::str::FromStr;
grammar;

pub Term: f64 = {
	//Boolean,
	Number,
	"(" <Term> ")",
};

// TODO: these need to be pub for testing but not for the actual build
pub Boolean: bool = r"true|false" => bool::from_str(<>).unwrap();
pub Number: f64 = r"-?(?:\d+e-?\d+|\d+\.?\d*|\.\d+)" => f64::from_str(<>).unwrap();